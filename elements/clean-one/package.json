    "@lrnwebcomponents/scroll-button": "^4.1.1",
    "@lrnwebcomponents/simple-colors": "^4.1.1",
    "@lrnwebcomponents/simple-icon": "^4.1.1",
    "@lrnwebcomponents/simple-popover": "^4.1.1",
    "@lrnwebcomponents/utils": "^4.1.1",
    "lit": "^2.6.1",
    "mobx": "6.7.0"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^4.0.0",
