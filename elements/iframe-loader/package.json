{
  "name": "@lrnwebcomponents/iframe-loader",
  "wcfactory": {
    "className": "IframeLoader",
    "customElementClass": "LitElement",
    "elementName": "iframe-loader",
    "generator-wcfactory-version": "0.7.4",
    "useHAX": false,
    "useSass": false,
    "files": {
      "js": "src/iframe-loader.js"
    }
  },
  "version": "7.0.12",
  "description": "Adds a loading indicator for iframes.",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "iframe-loader.js",
  "module": "iframe-loader.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"{*,lib/**}.js\" --format vscode --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.mjs",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox"
  },
  "author": {
    "name": "heymp"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@polymer/polymer": "^3.5.1",
    "lit": "^2.7.5"
  },
  "devDependencies": {
    "@open-wc/testing": "3.2.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.2.3",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "8.2.0",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
