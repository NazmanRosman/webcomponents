{
  "name": "@haxtheweb/documentation-player",
  "wcfactory": {
    "className": "DocumentationPlayer",
    "customElementClass": "LitElement",
    "elementName": "documentation-player",
    "generator-wcfactory-version": "0.13.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/documentation-player.css",
      "html": "src/documentation-player.html",
      "js": "src/documentation-player.js",
      "properties": "src/documentation-player-properties.json",
      "hax": "src/documentation-player-hax.json"
    },
    "iconset": {},
    "sharedHaxProps": [],
    "sharedProps": [],
    "sharedStyles": []
  },
  "version": "9.0.21",
  "description": "playlist like element but for documenting web components in the context of hax",
  "repository": {
    "type": "git",
    "url": "https://github.com/haxtheweb/webcomponents.git"
  },
  "main": "documentation-player.js",
  "module": "documentation-player.js",
  "scripts": {
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs  --playwright --browsers chromium",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --watch --config=../../web-test-runner.config.mjs  --playwright --browsers chromium",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}' && wca analyze \"*.js|lib/**.js\" --format vscode --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.mjs",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "haxtheweb",
    "url": "https://hax.psu.edu/"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@haxtheweb/code-editor": "^9.0.18",
    "@haxtheweb/code-sample": "^9.0.18",
    "@haxtheweb/play-list": "^9.0.21",
    "@haxtheweb/simple-icon": "^9.0.18",
    "@haxtheweb/simple-img": "^9.0.18",
    "@haxtheweb/utils": "^9.0.18",
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@haxtheweb/deduping-fix": "^9.0.18",
    "@open-wc/testing": "4.0.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.4.6",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "8.2.2",
    "gulp-babel": "8.0.0",
    "polymer-build": "3.1.4",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "customElements": "custom-elements.json",
  "keywords": [
    "webcomponents",
    "html",
    "haxtheweb",
    "lit"
  ],
  "gitHead": "73a678e1d035b003097fd50b7b23c0d9c30f9b26"
}
