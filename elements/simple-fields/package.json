{
  "name": "@lrnwebcomponents/simple-fields",
  "wcfactory": {
    "className": "SimpleFields",
    "customElementClass": "LitElement",
    "elementName": "simple-fields",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/simple-fields.css",
      "scss": "src/simple-fields.scss",
      "html": "src/simple-fields.html",
      "js": "src/simple-fields.js",
      "properties": "src/simple-fields-properties.json",
      "hax": "src/simple-fields-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "7.0.11",
  "description": "Uses JSON Schema or an array of fields to display a series of fields",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "simple-fields.js",
  "module": "simple-fields.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"{*,lib/**}.js\" --format vscode --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.mjs",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/a11y-tabs": "^7.0.11",
    "@lrnwebcomponents/code-editor": "^7.0.11",
    "@lrnwebcomponents/hax-iconset": "^7.0.11",
    "@lrnwebcomponents/i18n-manager": "^7.0.5",
    "@lrnwebcomponents/responsive-utility": "^7.0.11",
    "@lrnwebcomponents/simple-colors": "^7.0.11",
    "@lrnwebcomponents/simple-icon": "^7.0.11",
    "@lrnwebcomponents/simple-icon-picker": "^7.0.11",
    "@lrnwebcomponents/simple-login": "^7.0.11",
    "@lrnwebcomponents/simple-picker": "^7.0.11",
    "@lrnwebcomponents/simple-toolbar": "^7.0.11",
    "@lrnwebcomponents/simple-tooltip": "^7.0.11",
    "@lrnwebcomponents/utils": "^7.0.4",
    "@lrnwebcomponents/voice-recorder": "^7.0.11",
    "@vaadin/vaadin-upload": "^4.4.1",
    "lit": "^2.7.6"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^7.0.4",
    "@open-wc/testing": "3.2.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.2.3",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "8.2.0",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ],
  "gitHead": "44f96fea963c665e84698e2d358ef1fba1f0c29f"
}
