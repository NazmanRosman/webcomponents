    "elementName": "simple-drawer",
    "generator-wcfactory-version": "0.5.0",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/simple-drawer.css",
      "scss": "src/simple-drawer.scss",
      "html": "src/simple-drawer.html",
      "js": "src/simple-drawer.js",
