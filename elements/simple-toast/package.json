{
  "name": "@lrnwebcomponents/simple-toast",
  "wcfactory": {
    "className": "SimpleToast",
    "customElementClass": "LitElement",
    "elementName": "simple-toast",
    "generator-wcfactory-version": "0.6.3",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/simple-toast.css",
      "scss": "src/simple-toast.scss",
      "html": "src/simple-toast.html",
      "js": "src/simple-toast.js",
      "properties": "src/simple-toast-properties.json",
      "hax": "src/simple-toast-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "7.0.17",
  "description": "A singular toast / message for conistency",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "simple-toast.js",
  "module": "simple-toast.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"{*,lib/**}.js\" --format vscode --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.mjs",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/simple-colors": "^7.0.17",
    "lit": "^2.8.0"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^7.0.17",
    "@lrnwebcomponents/storybook-utilities": "^7.0.17",
    "@open-wc/testing": "3.2.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.3.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "8.2.0",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ],
  "gitHead": "a0c8558d0ed97b0ebbdddc84ea90fd01cf330635"
}
