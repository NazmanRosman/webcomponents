{
  "name": "@haxtheweb/a11y-gif-player",
  "wcfactory": {
    "className": "A11yGifPlayer",
    "customElementClass": "LitElement",
    "elementName": "a11y-gif-player",
    "generator-wcfactory-version": "0.3.1",
    "useHAX": true,
