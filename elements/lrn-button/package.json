{
  "name": "@haxtheweb/lrn-button",
  "wcfactory": {
    "className": "LrnButton",
    "customElementClass": "LitElement",
    "elementName": "lrn-button",
    "generator-wcfactory-version": "0.5.0",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/lrn-button.css",
      "scss": "src/lrn-button.scss",
      "html": "src/lrn-button.html",
      "js": "src/lrn-button.js",
      "properties": "src/lrn-button-properties.json",
      "hax": "src/lrn-button-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "9.0.1",
  "description": "Button element for PolymerElement",
  "repository": {
    "type": "git",
    "url": "https://github.com/haxtheweb/webcomponents.git"
  },
  "type": "module",
  "main": "lrn-button.js",
  "module": "lrn-button.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"{*,lib/**}.js\" --format vscode --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.mjs",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium"
  },
  "author": {
    "name": "haxtheweb",
    "url": "https://hax.psu.edu/"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@haxtheweb/simple-icon": "^9.0.1",
    "@haxtheweb/simple-tooltip": "^9.0.1",
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "@haxtheweb/deduping-fix": "^9.0.1",
    "@haxtheweb/storybook-utilities": "^9.0.1",
    "@open-wc/testing": "4.0.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.4.2",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "8.2.2",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "7f62cde6a6242afcb57715e1e2067ea767c777fd"
